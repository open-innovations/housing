<h3>Total sites<a id='total-sites' class='anchor' href='#total-sites' data-dependencies='/assets/js/section-links.js'>ยง</a></h3>

{{ comp.oi.map.hex_cartogram({
	config: {
		width: 600,
		hexjson: "hexjson.uk-local-authority-districts-2023",
		data: brownfield(),
		scale: 'Viridis',
		columns: [{
				name: 'filterLabel',
				template: "{{ n }}"
			}],
		matchKey: "LAD24CD",
		value: "sites",
		min: 0,
		max: 1000,
		legend: { 
			position: "top right",
			items: [{
				value: 1000,
				label: '1000+'
