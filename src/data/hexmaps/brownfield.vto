				{
				value: 500,
				label: '500'
				},
				{
				value: 0,
				label: '0'
				}] 
			},
		tools: {
			filter: {
				label: "filterLabel",
				position: "top left"
			}
		},
		boundaries: {
			country: {
				stroke: "#181b25",
				"stroke-width": 2,
				"stroke-linecap": "round",
				"stroke-opacity": 0.5
				},
			regions: {
				stroke: "#181b25",
				"stroke-width": 2,
				"stroke-linecap": "round",
				"stroke-opacity": 0.5
			}
			},
		tools: { 
			filter: {
				label: "filterLabel",
				position: "top left"
				}
			},
		tooltip: "<strong class='subtitle'>{{ n }}</strong><br />Brownfield sites: <strong>{{ sites }}</strong>",
		}
}) }}

